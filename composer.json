{
    "name": "calebegmsp/gu-payment",
    "description": "GuPayment fornece uma interface para controlar assinaturas do iugu.com",
    "keywords": ["laravel", "iugu", "billing", "pagamentos"],
    "license": "MIT",
    "authors": [
        {
            "name": "Calebe Pereira",
            "email": "calebepereira.tec@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "calebegmsp/iugu": "master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:calebegmsp/iugu-php.git"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "7.x"
    },
    "autoload": {
        "psr-4": {
            "Potelo\\GuPayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Potelo\\GuPayment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Potelo\\GuPayment\\GuPaymentServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "scripts": {
      "test": [
        "Composer\\Config::disableProcessTimeout",
        "phpunit"
      ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
